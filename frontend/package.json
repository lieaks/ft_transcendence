    "pinia": "^2.1.4",
    "pinia-plugin-persistedstate": "^3.1.0",
    "socket.io-client": "^4.7.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vue3-toastify": "^0.1.11"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^20.3.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
